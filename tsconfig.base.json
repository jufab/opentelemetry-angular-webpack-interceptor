{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "esModuleInterop": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "opentelemetry-interceptor-webpack": [
        "dist/opentelemetry-interceptor-webpack/opentelemetry-interceptor-webpack",
        "dist/opentelemetry-interceptor-webpack"
      ]
    }
  }
}
